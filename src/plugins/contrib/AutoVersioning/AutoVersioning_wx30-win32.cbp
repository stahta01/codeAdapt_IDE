<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="AutoVersioning wx3.0.x win32" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/codeadapt/plugins/AutoVersioning.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs30/plugins/contrib/AutoVersioning" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="../../../devel30/codeadapt" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-std=gnu++11" />
					<Add option="-iquote$(CA_OBJ)/include" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCA_USEPCH" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="../../../include" />
					<Add directory="$(#WX30.include)" />
					<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
				</Compiler>
				<Linker>
					<Add library="codeadapt" />
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)_adv" />
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)_core" />
					<Add library="wxbase$(WX_VERSION)$(WX_SUFFIX)" />
					<Add directory="../../../devel30" />
					<Add directory="$(#WX30.lib)/gcc_dll$(WX_CFG)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel30\share\codeadapt\AutoVersioning.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="CA_OBJ" value="..\..\..\.objs30" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
		</Compiler>
		<Unit filename="AutoVersioning.cpp" />
		<Unit filename="AutoVersioning.h" />
		<Unit filename="avChangesDlg.cpp" />
		<Unit filename="avChangesDlg.h" />
		<Unit filename="avConfig.h" />
		<Unit filename="avHeader.cpp" />
		<Unit filename="avHeader.h" />
		<Unit filename="avSvnRevision.cpp" />
		<Unit filename="avSvnRevision.h" />
		<Unit filename="avVersionEditorDlg.cpp" />
		<Unit filename="avVersionEditorDlg.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<wxsmith version="1">
				<gui name="wxWidgets" src="avVersionEditorDlg.cpp" main="avVersionEditorDlg" init_handlers="necessary" language="CPP" />
				<resources>
					<wxDialog wxs="wxsmith/avVersionEditorDlg.wxs" src="avVersionEditorDlg.cpp" hdr="avVersionEditorDlg.h" name="avVersionEditorDlg" language="CPP" />
					<wxDialog wxs="wxsmith/avChangesDlg.wxs" src="avChangesDlg.cpp" hdr="avChangesDlg.h" name="avChangesDlg" language="CPP" />
				</resources>
			</wxsmith>
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
