<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Profiler wx3.0.x win32" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/codeadapt/plugins/Profiler.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs30/plugins/contrib/profiler" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="../../../devel30/codeadapt.exe" />
				<Option run_host_application_in_terminal="0" />
			</Target>
			<Environment>
				<Variable name="CA_OBJ" value="..\..\..\.objs30" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_COMPILER" value="471TDM" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-Winvalid-pch" />
			<Add option="-mthreads" />
			<Add option="-fexceptions" />
			<Add option="-std=gnu++11" />
			<Add option="-iquote$(CA_OBJ)/include" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DCA_USEPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="../../../include" />
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc$(WX_COMPILER)_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add library="codeadapt" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)_xrc" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)_core" />
			<Add library="wxbase$(WX_VERSION)$(WX_SUFFIX)" />
			<Add directory="../../../devel30" />
			<Add directory="$(#WX30.lib)/gcc$(WX_COMPILER)_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel30\share\codeadapt\Profiler.zip resources\manifest.xml resources\*.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="cbprofiler.cpp" />
		<Unit filename="cbprofiler.h" />
		<Unit filename="cbprofilerconfig.cpp" />
		<Unit filename="cbprofilerconfig.h" />
		<Unit filename="cbprofilerexec.cpp" />
		<Unit filename="cbprofilerexec.h" />
		<Unit filename="resources/configuration.xrc" />
		<Unit filename="resources/main_dialog.xrc" />
		<Unit filename="resources/manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
