<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CopyStrings wx2.8.x win32" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel/share/CodeBlocks/plugins/copystrings.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs28/plugins/contrib/copystrings" />
				<Option type="3" />
				<Option compiler="gcc" />
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="28" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DWX_PRECOMP" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="$(#WX.include)" />
			<Add directory="$(#WX.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="$(#WX)/contrib/include" />
			<Add directory="../../../include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add library="libwxscintilla.a" />
			<Add library="codeadapt" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)_core" />
			<Add library="wxbase$(WX_VERSION)$(WX_SUFFIX)" />
			<Add directory="../../../devel" />
			<Add directory="$(#WX.lib)/gcc_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel\share\CodeBlocks\copystrings.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="copystrings.cpp" />
		<Unit filename="copystrings.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
