<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="RegExTestbed wx3.0.x win32" />
		<Option pch_mode="2" />
		<Option default_target="default" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/codeadapt/plugins/RegExTestbed" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs30/plugins/contrib/regex_testbed" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="../../../devel30/codeadapt.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DcbDEBUG" />
				</Compiler>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel30\share\codeadapt\RegExTestbed.zip manifest.xml *.xrc" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="CA_OBJ" value="..\..\..\.objs30" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_COMPILER" value="471TDM" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-pipe" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-mthreads" />
			<Add option="-std=gnu++11" />
			<Add option="-iquote$(CA_OBJ)/include" />
			<Add option="-DCA_USEPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DSUPPORT_UTF8" />
			<Add directory="$(#WX30.include)" />
			<Add directory="$(#WX30.lib)/gcc$(WX_COMPILER)_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="../../../include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX30.include)" />
		</ResourceCompiler>
		<Linker>
			<Add library="codeadapt" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)_xrc" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)_core" />
			<Add library="wxbase$(WX_VERSION)$(WX_SUFFIX)" />
			<Add directory="../../../devel30" />
			<Add directory="$(#WX30.lib)/gcc$(WX_COMPILER)_dll$(WX_CFG)" />
		</Linker>
		<Unit filename="RegExTestbed.cpp" />
		<Unit filename="RegExTestbed.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="regexdlg.cpp" />
		<Unit filename="regexdlg.h" />
		<Extensions>
			<code_completion />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxDialog wxs="wxsmith/RegExDlg.wxs" src="regexdlg.cpp" hdr="regexdlg.h" xrc="regexdlg.xrc" name="regex_dialog" language="CPP" />
				</resources>
			</wxsmith>
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
