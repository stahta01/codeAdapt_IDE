<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Code Stat wx2.8.x win32" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel/share/codeadapt/plugins/codestat.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs28/plugins/contrib/codestat" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="../../../devel/codeadapt.exe" />
				<Option run_host_application_in_terminal="0" />
				<Option projectCompilerOptionsRelation="0" />
			</Target>
			<Environment>
				<Variable name="CA_OBJ" value="../../../.objs28" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_COMPILER" value="gcc" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_TOOLKIT" value="msw" />
				<Variable name="WX_VERSION" value="28" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-Winvalid-pch" />
			<Add option="-mthreads" />
			<Add option="-fexceptions" />
			<Add option="-std=gnu++11" />
			<Add option="-iquote$(CA_OBJ)/include" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DCA_USEPCH" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="../../../include" />
			<Add directory="$(#WX.INCLUDE)" />
			<Add directory="$(#WX.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add library="codeadapt" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add directory="../../../devel" />
			<Add directory="$(#WX.lib)/$(WX_COMPILER)_dll$(WX_CFG)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel\share\codeadapt\codestat.zip resources\manifest.xml resources\*.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="codestat.cpp" />
		<Unit filename="codestat.h" />
		<Unit filename="codestatconfig.cpp" />
		<Unit filename="codestatconfig.h" />
		<Unit filename="codestatexec.cpp" />
		<Unit filename="codestatexec.h" />
		<Unit filename="language_def.h" />
		<Unit filename="resources/config_dialog.xrc" />
		<Unit filename="resources/main_dialog.xrc" />
		<Unit filename="resources/manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
